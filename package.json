{
  "name": "point-of-sale",
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "debug": "nodemon src/app.js",
    "start": "node src/app.js",
    "create": "npx sequelize-cli db:create",
    "drop": "npx sequelize-cli db:drop",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "migrate:undo:all": "npx sequelize-cli db:migrate:undo:all",
    "seed": "npx sequelize-cli db:seed",
    "seed:all": "npx sequelize-cli db:seed:all",
    "seed:undo": "npx sequelize-cli db:seed:undo",
    "seed:undo:all": "npx sequelize-cli db:seed:undo:all",
    "refreshDB": "npx sequelize-cli db:drop && npx sequelize-cli db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all"
  },
  "author": "Kyaw Min Htwe",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-add-module-exports": "^1.0.4",
    "babel-preset-es2015": "^6.24.1",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-async-errors": "^3.1.1",
    "helmet": "^8.0.0",
    "moment": "^2.30.1",
    "pg": "^8.13.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.5",
    "sequelize-cli": "^6.6.2",
    "uuid": "^11.0.3",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.9"
  }
}
